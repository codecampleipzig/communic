{
  "name": "communic",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node server.js",
    "dev": "ng serve",
    "build": "ng build --prod",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand",
    "test:coverage": "jest --coverage",
    "lint": "ng lint",
    "autofix": "ng lint --fix",
    "compodoc": "compodoc -p tsconfig.app.json --coverageTest 70 --coverageMinimumPerFile 20 --coverageTestThresholdFail false --coverageTestShowOnlyFailed"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run lint && npm test && npm run compodoc"
    }
  },
  "lint-staged": {
    "*.{ts,json}": [
      "prettier --write '**/*.{ts,json}'",
      "git add"
    ],
    "*.ts": [
      "compodoc --coverageTest 70 --coverageMinimumPerFile 20 --coverageTestThresholdFail false --coverageTestShowOnlyFailed"
    ]
  },
  "private": true,
  "main": "main.js",
  "dependencies": {
    "@angular/animations": "~8.2.0",
    "@angular/common": "~8.2.0",
    "@angular/compiler": "~8.2.0",
    "@angular/core": "~8.2.0",
    "@angular/forms": "~8.2.0",
    "@angular/platform-browser": "~8.2.0",
    "@angular/platform-browser-dynamic": "~8.2.0",
    "@angular/router": "~8.2.0",
    "@fortawesome/fontawesome-free": "^5.11.2",
    "axios": "^0.19.0",
    "express": "^4.17.1",
    "rxjs": "~6.5.3",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.18",
    "@angular/cli": "^8.3.18",
    "@angular/compiler-cli": "~8.2.0",
    "@angular/language-service": "~8.2.0",
    "@compodoc/compodoc": "^1.1.11",
    "@types/jest": "^24.0.21",
    "@types/node": "~8.9.4",
    "canvas": "^2.6.0",
    "codelyzer": "^5.0.0",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "jest-preset-angular": "^8.0.0",
    "lint-staged": "^9.4.3",
    "ng-lint-staged": "^0.1.6",
    "prettier": "^1.19.1",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codecampleipzig/communic.git"
  },
  "author": "CCL",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/codecampleipzig/communic/issues"
  },
  "homepage": "https://github.com/codecampleipzig/communic#readme"
}
